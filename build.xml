<project name="titaniumqr" default="dist">
  <description>
    Ant build script for Titanium Android module tiqr
  </description>

  <!-- override some Titanium defaults so they're more Maven-like -->
  <property name="ti.module.root" location="${basedir}"/>
  <property name="src" location="${ti.module.root}/src/main/java"/>
  <property name="assets" location="${ti.module.root}/src/main/resources"/>

  <property file="build.properties" />

  <import file="${titanium.platform}/../module/android/build.xml"/>
</project>
